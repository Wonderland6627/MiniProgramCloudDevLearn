<!--pages/nbstudy/admin-edit/index.wxml-->
<text>pages/nbstudy/admin-edit/index.wxml</text>

<view class="scrollview">
  <scroll-view scroll-y="true" style="height: 100%;">
    <view class="cell" wx:for="{{students}}" wx:key="index">
      <view class="avatar-container">
        <image class="avatar" src="{{item.avatarUrl}}"></image>
      </view>
      <view class="info">
        <view class="name">{{item.studentName}}</view>
        <!-- <view class="message">{{item.gender}}</view> -->
        <view class="message">{{item.school}}</view>
      </view>
    </view>
  </scroll-view>
</view>