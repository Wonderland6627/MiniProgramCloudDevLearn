/* components/inputWithTitle/index.wxss */
.container {
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.i-cell{
  position: relative;
  padding: 24rpx 30rpx;
  display: flex;
  background: #fff;
  align-items: center;
  line-height: 1.2;
  font-size: 28rpx;
  overflow: hidden;
}

.title {
  min-width: 130rpx;
  padding-right: 20rpx;
}

.input {
  flex: 1;
  padding: 8rpx 0;
}